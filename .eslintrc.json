{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:jsdoc/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "process": "readonly"
  },
  "plugins": ["jsdoc"],
  "rules": {
    "jsdoc/tag-lines": 0
  }
}
